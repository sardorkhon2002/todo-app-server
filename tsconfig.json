{
  "compilerOptions": {
    "lib": ["es2019","es5", "es6", "dom"],
    "baseUrl": "./src",
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "noEmit": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
